# Server Configuration
NODE_ENV=development
PORT=8000
HOST=localhost

# Database
DATABASE_URL=postgresql://user:password@localhost:5432/aistm7
DATABASE_SSL=false

# Authentication
JWT_SECRET=your-jwt-secret-here
JWT_EXPIRY=24h

# IBKR Web API
IBKR_API_URL=https://api.ibkr.com/v1/portal
IBKR_AUTH_URL=https://api.ibkr.com/v1/oauth/authorize
IBKR_TOKEN_URL=https://api.ibkr.com/v1/oauth/token
IBKR_OAUTH_CLIENT_ID=your-oauth-client-id
IBKR_OAUTH_CLIENT_SECRET=your-oauth-client-secret
IBKR_OAUTH_REDIRECT_URI=http://localhost:3000/api/auth/callback/ibkr

# Solana Configuration
SOLANA_NETWORK=devnet
SOLANA_RPC_URL=https://api.devnet.solana.com
SOLANA_PRIVATE_KEY=your-solana-private-key
TOKEN_CONTRACT_ADDRESS=your-token-contract-address

# AI/ML Services
TENSORFLOW_SERVING_URL=http://localhost:8501
MODEL_CHECKPOINT_DIR=./models/checkpoints
EXPERIMENT_TRACKING_DIR=./experiments

# Monitoring & Logging
LOG_LEVEL=debug
SENTRY_DSN=your-sentry-dsn
DATADOG_API_KEY=your-datadog-api-key
METRICS_PORT=9090

# Redis Cache
REDIS_URL=redis://localhost:6379
REDIS_PASSWORD=your-redis-password

# Email Service
SMTP_HOST=smtp.example.com
SMTP_PORT=587
SMTP_USER=your-smtp-username
SMTP_PASSWORD=your-smtp-password
EMAIL_FROM=noreply@aistm7.com

# Security
CORS_ORIGIN=http://localhost:3000
RATE_LIMIT_WINDOW=15m
RATE_LIMIT_MAX=100
ENCRYPTION_KEY=your-encryption-key

# Feature Flags
ENABLE_WEBSOCKETS=true
ENABLE_MONITORING=true
ENABLE_CACHING=true
ENABLE_AI_SERVICES=true

# Backup Configuration
BACKUP_ENABLED=true
BACKUP_INTERVAL=24h
BACKUP_RETENTION_DAYS=30
BACKUP_S3_BUCKET=your-backup-bucket

# Performance
MAX_CONCURRENT_CALCULATIONS=10
CACHE_TTL=3600
WEBSOCKET_HEARTBEAT_INTERVAL=30000

# API Limits
MAX_REQUEST_SIZE=10mb
MAX_PORTFOLIO_ITEMS=1000
MAX_CONCURRENT_SESSIONS=100